MnbToolbarButton {
  background-color: #fff0;
}

MnbToolbarButton:checked {
  border-image: url("@PKGTHEMEDIR@/toolbar-button-active.png") 0 stretch;
}

MnbToolbarButton:hover, MnbToolbarButton:active {
  border-image: url("@PKGTHEMEDIR@/toolbar-button-hover.png") 0 stretch;
}

MnbToolbarButton#state-25 {
  background-image: url("@PKGTHEMEDIR@/battery-25-normal.png");
}

MnbToolbarButton#state-25:hover, MnbToolbarButton#state-25:active, MnbToolbarButton#state-25:checked  {
  background-image: url("@PKGTHEMEDIR@/battery-25-active.png");
}

