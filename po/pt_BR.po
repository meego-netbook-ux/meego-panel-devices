#: ../gvc/gvc-mixer-control.c:1005
msgid "Disabled"
msgstr "Desabilitado"

#. translators:
#. * The number of sound outputs on a particular device
#: ../gvc/gvc-mixer-control.c:1012
#, c-format
msgid "%u Output"
msgid_plural "%u Outputs"
msgstr[0] "%u saída"
msgstr[1] "%u saídas"

#. translators:
#. * The number of sound inputs on a particular device
#: ../gvc/gvc-mixer-control.c:1022
#, c-format
msgid "%u Input"
msgid_plural "%u Inputs"
