#: ../dalston/dalston-power-applet.c:237
#, c-format
msgid "<b>%d</b> hour"
msgid_plural "<b>%d</b> hours"
msgstr[0] "<b>%d</b> heure"
msgstr[1] "<b>%d</b> heures"

#: ../dalston/dalston-power-applet.c:244
#: ../dalston/dalston-power-applet.c:257
#, c-format
