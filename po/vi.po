msgid "devices"
msgstr "thiết bị"

#: ../data/meego-panel-devices.schemas.in.h:1
msgid "Sleep timeout"
msgstr "Hết thời gian ngủ"

#: ../data/meego-panel-devices.schemas.in.h:2
msgid ""
"Value of -1 means never sleep, other values are seconds of continuous idle "
"state before sleeping."
msgstr ""
"Giá trị 1 là không bao giờ ngủ, các giá trị khác là khoảng thời gian máy "
"nhàn rỗi trước khi đi ngủ."

#. translators:
#. * The device has been disabled
#: ../gvc/gvc-mixer-control.c:1005
msgid "Disabled"
msgstr "Bị khóa"

#. translators:
#. * The number of sound outputs on a particular device
#: ../gvc/gvc-mixer-control.c:1012
#, c-format
msgid "%u Output"
msgid_plural "%u Outputs"
msgstr[0] "%u Đầu ra"

#. translators:
#. * The number of sound inputs on a particular device
#: ../gvc/gvc-mixer-control.c:1022
#, c-format
msgid "%u Input"
msgid_plural "%u Inputs"
