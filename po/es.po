msgstr "Ha amanecido"

# midday
#: ../dalston/dalston-brightness-slider.c:229
msgid "Midday"
msgstr "Mediodía"

# rather dark
#: ../dalston/dalston-brightness-slider.c:231
msgid "Pretty dusky"
msgstr "Más bien oscuro"

# It's getting dark
#: ../dalston/dalston-brightness-slider.c:233
msgid "Twilight"
msgstr "Anocheciendo"

# very dark
#: ../dalston/dalston-brightness-slider.c:235
msgid "Very dark"
msgstr "Muy oscuro"

# <meaning very dark>
# Total eclipse
#: ../dalston/dalston-brightness-slider.c:237
msgid "Total eclipse"
msgstr "Eclipse total"

