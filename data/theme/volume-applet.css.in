MnbToolbarButton {
  background-color: #fff0;
}

MnbToolbarButton:checked {
  border-image: url("file://@pkgdatadir@/icons/toolbar-button-active.png") 0 stretch;
}

MnbToolbarButton:hover, MnbToolbarButton:active {
  border-image: url("file://@pkgdatadir@/icons/toolbar-button-hover.png") 0 stretch;
}

MnbToolbarButton#state-0 {
  background-image: url("file://@pkgdatadir@/icons/dalston-volume-applet-0-normal.png");
}

MnbToolbarButton#state-0:hover, MnbToolbarButton#state-0:active, MnbToolbarButton#state-0:checked  {
  background-image: url("file://@pkgdatadir@/icons/dalston-volume-applet-0-active.png");
}

MnbToolbarButton#state-1 {
  background-image: url("file://@pkgdatadir@/icons/dalston-volume-applet-1-normal.png");
}

MnbToolbarButton#state-1:hover, MnbToolbarButton#state-1:active, MnbToolbarButton#state-1:checked  {
  background-image: url("file://@pkgdatadir@/icons/dalston-volume-applet-1-active.png");
}

MnbToolbarButton#state-2 {
  background-image: url("file://@pkgdatadir@/icons/dalston-volume-applet-2-normal.png");
}

MnbToolbarButton#state-2:hover, MnbToolbarButton#state-2:active, MnbToolbarButton#state-2:checked  {
  background-image: url("file://@pkgdatadir@/icons/dalston-volume-applet-2-active.png");
}

MnbToolbarButton#state-3 {
  background-image: url("file://@pkgdatadir@/icons/dalston-volume-applet-3-normal.png");
}

MnbToolbarButton#state-3:hover, MnbToolbarButton#state-3:active, MnbToolbarButton#state-3:checked  {
  background-image: url("file://@pkgdatadir@/icons/dalston-volume-applet-3-active.png");
}

